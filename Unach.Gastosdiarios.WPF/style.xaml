<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Colores-->
    <Color x:Key="ColorAzul">#1e97b5</Color>
    <Color x:Key="ColorAreaTitulo">white</Color>
    <Color x:Key="ColorGris">#9c9b9b</Color>
    <Color x:Key="ColorContenido">#d7d7d7</Color>
    <!--TextBlock-->
    <Style TargetType="TextBlock">
        <Setter Property="FontSize" Value="12"/>
        <Setter Property="Foreground" Value="Gray"/>
        <Setter Property="FontFamily" Value="Century Gothic"/>
    </Style>
    <Style x:Key="TituloTextBlock" TargetType="TextBlock">
        <Setter Property="FontSize" Value="30"/>
        <Setter Property="Foreground" Value="#1e97b5"/>
        <Setter Property="FontFamily" Value="Century Gothic"/>
    </Style>
    <Style x:Key="SubTituloTextBlock" TargetType="TextBlock">
        <Setter Property="FontSize" Value="30"/>
        <Setter Property="Foreground" Value="Gray"/>
        <Setter Property="FontWeight" Value="Bold"/>
        <Setter Property="FontFamily" Value="Century Gothic"/>
        <Setter Property="TextAlignment" Value="Center"/>
    </Style>
</ResourceDictionary>